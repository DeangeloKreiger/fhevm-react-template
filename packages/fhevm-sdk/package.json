{
  "name": "fhevm-sdk",
  "version": "1.0.0",
  "description": "Universal SDK for building confidential frontends with FHEVM",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/vue/index.mjs",
      "require": "./dist/vue/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "build:react": "tsup src/react/index.ts --format cjs,esm --dts --outDir dist/react",
    "build:vue": "tsup src/vue/index.ts --format cjs,esm --dts --outDir dist/vue",
    "build:all": "pnpm build && pnpm build:react && pnpm build:vue",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "zama",
    "confidential",
    "privacy",
    "blockchain",
    "ethereum",
    "web3",
    "sdk"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "fhevmjs": "^0.5.2",
    "ethers": "^6.13.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/fhevm-react-template.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/fhevm-react-template/issues"
  },
  "homepage": "https://github.com/yourusername/fhevm-react-template#readme"
}
